   "source": [
    "st = str(input(\"Please enter a string\"))\n",
    "j = -1\n",
    "flag = 0\n",
    "for i in st:\n",
    "    if i != st[j]:\n",
    "        flag = 1\n",
    "        break\n",
    "    j = j - 1\n",
    "if flag == 1:\n",
    "    print(\"NO it is not a Palindrome\")\n",
    "else:\n",
    "    print(\"Yes it is a Palindrome\")"
